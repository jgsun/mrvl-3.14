/dts-v1/;
#include "pxa1936-board-common.dtsi"

/ {
	model = "Marvell PXA1936 Aloe V01 Development Board";
	compatible = "marvell,pxa1936-dkb", "marvell,pxa1936";
	soc {
		apb@d4000000 {
			twsi1: i2c@d4010800 {
				touch1: s3202@720p {
					avdd-supply = <&ldo6>;
				};

				touch2: s3202@1080p {
					avdd-supply = <&ldo6>;
				};
			};

			twsi2: i2c@d4037000 {
				status = "okay";

				pmic0: 88pm880@30 {
					reg = <0x30>;
					interrupts = <0 4 IRQ_TYPE_LEVEL_HIGH>;
					interrupt-parent = <&gic>;

					dvc {
						pinctrl-names = "default";
						pinctrl-0 = <&dvc_pmx_func>;
					};

					/*
					 * TODO: remove later, fake ldo,
					 * to pass compile
					 */
					ldo19: regulator-ldo19 {
					};
				};

				pmic2: 88pm860@38 {
					compatible = "marvell,88pm860";
					reg = <0x38>;
					marvell,pmic-type = <1>;
					/* no codec_int currently, comment out now */
					/*
					interrupt-parent = <&gpio>;
					interrupts = <124 0x1>;
					marvell,88pm805-irq-write-clear;
					*/
					pm860_codec: pm860_codec {
						compatible = "marvell,88pm860-codec";
						#dailink-cells = <1>;
					};
				};
			};
			twsi3: i2c@d4013800 {
				status = "okay";
				sensor1: apds9930@39 {
					avdd-supply = <&ldo16>;
					};
				sensor2: bmi160@68 {
					avdd-supply = <&ldo16>;
					};

				sensor3: bme280@76 {
					avdd-supply = <&ldo16>;
					};
				sensor4: icm20628@69 {
					avdd-supply = <&ldo16>;
					};
				sensor5: mxc400x@15 {
					avdd-supply = <&ldo16>;
					};
				sensor6: mmc3524x@30 {
					avdd-supply = <&ldo16>;
					};
			};
			map@d1200000 {
				pmic-name = "88pm880";
				vccmain-supply = <&buck1aslp>;
			};
		};

		axi@d4200000 {  /* AXI */
			/* SD card */
			sdh0: sdh@d4280000 {
			      vqmmc-supply    = <&ldo10>;
			      vmmc-supply     = <&ldo14>;
			};
		};
	};
	panel1: r63311 {
		rst_gpio = <&gpio 98 0>;
		power_gpio = <&gpio 96 0>;
		marvell,mipi-backlight;
	};
	panel2: hx8394 {
		rst_gpio = <&gpio 98 0>;
		avdd-supply = <&ldo6>;
	};

	panel3: lg4591 {
		rst_gpio = <&gpio 98 0>;
		avdd-supply = <&ldo6>;
	};

	mmp_m3_1: apsenhb {
		pmicver = <2>;
		vm3pwr-supply = <&ldo3>;
		antpwr-supply = <&ldo4>;
		vccmain-supply = <&buck1aslp>;
	};

	mmp_m3_2: cm3senhb {
		pmicver = <2>;
		vm3pwr-supply = <&ldo3>;
		antpwr-supply = <&ldo4>;
		senpwr-supply = <&ldo16>;
		vccmain-supply = <&buck1aslp>;
	};

	mcam0:mcam@0 {
		sc2-i2c-dyn-ctrl;
		pwdn-gpios = <&gpio 68 0>;
		reset-gpios = <&gpio 67 0>;
		/*
		af_2v8-supply = <&ldo10>;
		avdd_2v8-supply = <&ldo11>;
		dovdd_1v8-supply = <&voutsw>;
		dvdd_1v2-supply = <&ldo12>;
		*/
		dphy_val = <0xff1d00 0x00024733 0x04001001>;
		status = "disable";
	};

	mcam1:mcam@1 {
		sc2-i2c-dyn-ctrl;
		pwdn-gpios = <&gpio 70 0>;
		reset-gpios = <&gpio 69 0>;

		af_2v8-supply = <&ldo8>;
		avdd_2v8-supply = <&ldo7>;
		dovdd_1v8-supply = <&buck2>;
		dvdd_1v2-supply = <&ldo17>;

		dphy_val = <0xff1d00 0x00024733 0x04001001>;
		status = "okay";
	};

	soc-camera@0 {
		status = "okay";
	};

	soc-camera@1 {
		status = "okay";
	};

	plat_cam {
		status = "okay";
		frontsensor {
			compatible = "marvell,frontsensor";
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";
			OV2680f: sensor@0 {
				compatible = "ovt,ov2680";
				status = "okay";
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names =  "SC2MCLK";
				reg = <0x36>;
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <26000000>;
				dphy3 = <0x1900>;
				dphy5 = <0x4711>;
				dphy6 = <0x1001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 70 1>;
				reset-gpios = <&gpio 69 1>;
			};
                       S5K5E3F: sensor@1 {
                               compatible = "samsung,s5k5e3";
                               status = "okay";
                               reg = <0x10>;
                               clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
                               clock-names = "SC2MCLK";
                               sc2-i2c-dyn-ctrl;
                               adapter = <0x1>;
                               sensor-pos = <1>;
                               mclk = <26000000>;
                               dphy3 = <0x5100>;
                               dphy5 = <0xff>;
                               dphy6 = <0x1001>;
                               af_2v8-supply = <&ldo8>;
                               avdd_2v8-supply = <&ldo7>;
                               dovdd_1v8-supply = <&buck2>;
                               dvdd_1v2-supply = <&ldo17>;
                               pwdn-gpios = <&gpio 70 1>;
                               reset-gpios = <&gpio 69 1>;
                       };

		};
		backsensor {
			compatible = "marvell,backsensor";
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			OV5642: sensor@0 {
				compatible = "ovt,ov5642";
				sensor-name = "ovt,ov5642";
				reg = <0x3c>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				adapter = <0x1>;
				status = "okay";
				mclk = <24000000>;
				sensor-pos = <1>;
				dphy3 = <0x1500>;
				dphy5 = <0xff>;
				dphy6 = <0x1001>;
				/*
				af_2v8-supply = <&ldo10>;
				avdd_2v8-supply = <&ldo11>;
				dovdd_1v8-supply = <&voutsw>;
				dvdd_1v2-supply = <&ldo12>;
				*/
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			OV13850: sensor@1 {
				compatible = "ovt,ov13850";
				status = "okay";
				reg = <0x10>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <24000000>;
				dphy3 = <0xff4115>;
				dphy5 = <0x247ff>;
				dphy6 = <0x4001001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			SR544: sensor@2 {
				compatible = "samsung,sr544";
				status = "okay";
				reg = <0x28>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <26000000>;
				dphy3 = <0x1500>;
				dphy5 = <0xff>;
				dphy6 = <0x1001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			imx219: sensor@3 {
				compatible = "sony,imx219";
				status = "okay";
				reg = <0x1A>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <26000000>;
				dphy3 = <0x1500>;
				dphy5 = <0xff>;
				dphy6 = <0x1001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			OV13850R2A: sensor@4 {
				compatible = "ovt,ov13850r2a";
				status = "okay";
				reg = <0x10>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <24000000>;
				dphy3 = <0xff4116>;
				dphy5 = <0x247ff>;
				dphy6 = <0x4001001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			S5K3L2: sensor@5 {
				compatible = "samsung,s5k3l2";
				status = "okay";
				reg = <0x2d>;
				clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
				clock-names = "SC2MCLK";
				sc2-i2c-dyn-ctrl;
				adapter = <0x1>;
				sensor-pos = <1>;
				mclk = <26000000>;
				dphy3 = <0xff4115>;
				dphy5 = <0x247ff>;
				dphy6 = <0x4001001>;
				af_2v8-supply = <&ldo8>;
				avdd_2v8-supply = <&ldo7>;
				dovdd_1v8-supply = <&buck2>;
				dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
			S5K5E3B: sensor@6 {
                                compatible = "samsung,s5k5e3";
                                status = "okay";
                                reg = <0x10>;
                                clocks = <&soc_clocks PXA1936_CLK_SC2_MCLK>;
                                clock-names = "SC2MCLK";
                                sc2-i2c-dyn-ctrl;
                                adapter = <0x1>;
                                sensor-pos = <1>;
                                mclk = <26000000>;
                                dphy3 = <0x5100>;
                                dphy5 = <0xff>;
                                dphy6 = <0x1001>;
                                af_2v8-supply = <&ldo8>;
                                avdd_2v8-supply = <&ldo7>;
                                dovdd_1v8-supply = <&buck2>;
                                dvdd_1v2-supply = <&ldo17>;
				pwdn-gpios = <&gpio 68 1>;
				reset-gpios = <&gpio 67 1>;
			};
		};
	};

	b52isp_sc2_csi0 {
	        status = "okay";
	};
	b52isp_sc2_csi1 {
			csi_mux_repacked;
	        status = "okay";
	};
	b52isp_sc2_dma0 {
	        status = "okay";
	};
	b52isp_sc2_dma1 {
	        status = "okay";
	};
};

#include "88pm88x.dtsi"
#include "88pm880.dtsi"
#include "88pm88x_battery_common.dtsi"
#include "88pm88x_battery_485760ar.dtsi"
#include "88pm880_pxa1936_dkb.dtsi"
