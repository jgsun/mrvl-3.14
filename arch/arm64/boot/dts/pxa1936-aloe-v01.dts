/dts-v1/;
#include "pxa1936-board-common.dtsi"

&pmx {
	mfp_pins_group_0: mfp_pins_group_0 {
		pinctrl-single,pins = <
			DF_IO9 AF0
			DF_IO10 AF0
			DF_IO11 AF0
			DF_IO12 AF0
			DF_IO13 AF0
			DF_IO15 AF0
		>;
		DS_MEDIUM;PULL_DOWN;EDGE_NONE;LPM_NONE;
	};

	mfp_pins_group_2: mfp_pins_group_2 {
		pinctrl-single,pins = <
			GPIO12 AF0
			GPIO13 AF0
			GPIO14 AF0
			GPIO15 AF0
		>;
		DS_MEDIUM;PULL_UP;EDGE_NONE;LPM_NONE;
	};

	disp_func: disp_func {
		pinctrl-single,pins = <
			DF_IO8	AF4
		>;
		MFP_LPM_PULL_DW;
	};

	touch_pins_aloe: touch_pins_aloe {
		pinctrl-single,pins = <
			GPIO76 AF0
			GPIO78 AF0
			GPIO98 AF0
		 >;
		 MFP_LPM_FLOAT;
	 };

	sdh1_pmx_func3: sdh1_pmx_func3 {
		pinctrl-single,pins = <
			GPIO97 AF0
		>;
		DS_MEDIUM;PULL_FLOAT;EDGE_NONE;LPM_NONE;
	};

	sdh0_pmx_func1: sdh0_pmx_func1 {
		pinctrl-single,pins = <
			MMC1_DAT7 AF0
			MMC1_DAT6 AF0
			MMC1_DAT5 AF0
			MMC1_DAT4 AF0
			MMC1_WP AF1
			MMC1_CD AF1 /* Above pins not used by sdh1, configured as GPIO */
		>;
		MFP_DEFAULT;
	};

	usim1_pmx_func: usim1_pmx_func {
			pinctrl-single,pins = <
				GPIO50 AF0
			>;
			DS_FAST;PULL_UP;EDGE_NONE;LPM_NONE;
	};

	usim1_pmx_func_sleep: usim1_pmx_func_sleep {
			pinctrl-single,pins = <
				GPIO50 AF0
			>;
			DS_FAST;PULL_UP;EDGE_BOTH;LPM_NONE;
	};
	sensor_i2c_pmx: sensor_i2c_pmx {
		pinctrl-single,pins = <
			GPIO79 AF0
			GPIO80 AF0
		>;
		MFP_LPM_FLOAT;
	};
};

&disp {
	pinctrl-0 = <&disp_func>;
};

